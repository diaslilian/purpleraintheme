{
  "name": "diaslilian",
  "type": "dark",

  "colors": {
    // terminal
    "terminal.background": "#22172b",
    "terminal.foreground": "#ecddf3",
    "terminal.ansiBrightBlack": "#8c62a4",
    // contrast colors
    "contrastBorder": "#911ed43d",
    // base colors
    "focusBorder": "#9e50bade",
    "foreground": "#ecddf3",
    "selection.background": "#9e50bae5",
    // button control
    "button.background": "#3c2d46",
    "button.foreground": "#ecddf3",
    // dropdown control
    "dropdown.background": "#35253f",
    "dropdown.border": "#9d50ba",
    "dropdown.foreground": "#ecddf3",
    // input control
    "input.background": "#35253f",
    "input.foreground": "#ecddf3",
    "input.border": "#1d1921",
    "input.placeholderForeground": "#ad77c0",
    "inputOption.activeBorder": "#FF79C6",
    "inputValidation.infoBorder": "#a150be",
    "inputValidation.warningBorder": "#f59229",
    "inputValidation.errorBorder": "#ff0000",
    // badge
    "badge.background": "#4e445a",
    "badge.foreground": "#ecddf3",
    // progress bar
    "progressBar.background": "#b150d4",
    // list & trees
    "list.activeSelectionBackground": "#3a284d",
    "list.activeSelectionForeground": "#ecddf3",
    "list.dropBackground": "#633b72",
    "list.focusBackground": "#50445a75",
    "list.highlightForeground": "#8BE9FD",
    "list.hoverBackground": "#50445a75",
    "list.inactiveSelectionBackground": "#4e445a75",
    "list.warningForeground": "#f59229",
    "list.errorForeground": "#ff0000",
    // activity bar
    "activityBar.background": "#322441",
    "activityBar.inactiveForeground": "#815ea1",
    "activityBar.foreground": "#ecddf3",
    "activityBar.activeBorder": "#ff79c5d0",
    "activityBar.activeBackground": "#6436838c",
    "activityBarBadge.background": "#9420be",
    "activityBarBadge.foreground": "#ecddf3",
    // side bar
    "sideBar.background": "#201727",
    "sideBarTitle.foreground": "#ecddf3",
    "sideBarSectionHeader.background": "#6436838c",
    "sideBarSectionHeader.border": "#911ed465",
    // editor group & tabs
    "editorGroup.border": "#FF79C6",
    "editorGroup.dropBackground": "#55445a70",
    "editorGroupHeader.tabsBackground": "#3a284d",
    "tab.activeBackground": "#741297ee",
    "tab.activeForeground": "#ecddf3",
    "tab.border": "#911ed465",
    "tab.activeBorderTop": "#911ed465",
    "tab.inactiveBackground": "#32223f",
    "tab.inactiveForeground": "#ae8cd4",
    // panel colors
    "panel.background": "#6436838c",
    "panel.border": "#911ed465",
    "panelTitle.activeBorder": "#FF79C6",
    "panelTitle.activeForeground": "#ecddf3",
    "panelTitle.inactiveForeground": "#bea6da",
    // status bar color
    "statusBar.background": "#701192",
    "statusBar.foreground": "#ecddf3",
    "statusBar.debuggingBackground": "#ff0000e0",
    "statusBar.debuggingForeground": "#1e1921",
    "statusBar.noFolderBackground": "#1e1921",
    "statusBar.noFolderForeground": "#ecddf3",
    "statusBarItem.prominentBackground": "#ff0000",
    "statusBarItem.prominentHoverBackground": "#f59229",
    "statusBarItem.remoteForeground": "#ecddf3",
    "statusBarItem.remoteBackground": "#FF79C6",
    // editor colors
    "editor.background": "#110c16",
    "editor.foreground": "#ecddf3",
    "editorLineNumber.foreground": "#583e74",
    "editor.selectionBackground": "#4e445a",
    "editor.selectionHighlightBackground": "#424450",
    "editor.foldBackground": "#22172b",
    "editor.wordHighlightBackground": "#8be8fdb7",
    "editor.wordHighlightStrongBackground": "#50fa7ac7",
    "editorOverviewRuler.border": "#4e445a",
    "editorOverviewRuler.selectionHighlightForeground": "#f59229",
    "editorOverviewRuler.wordHighlightForeground": "#8be8fd",
    "editorOverviewRuler.wordHighlightStrongForeground": "#50fa7a"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6f547a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#e41fb9"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#9732db"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f85ab4"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#50FA7B"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#FF79C6"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#4393dd"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#F2AE72"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#F2BE22"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#ecddf3"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#ecddf3"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF79C6"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF79C6"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#53b5ce"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#f8a407"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b382ba"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF79C6"
      }
    },
